
  {
    "manifest_version": 2,

  "name": "Slack Channel Schedule Post",
  "description": "This extension is developed to post in specified channel On Slack in scheduled time.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "images/icon16.png",
    "default_popup": "index.html"
  },
  "background": {
    "scripts": ["js/jquery-2.1.1.js","js/vue.min.js","js/app.js","js/materialize.min.js"],
    "styles": ["css/materialize.min.css"]
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "http://fonts.googleapis.com/icon?family=Material+Icons"
  ]
}